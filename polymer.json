{
  "entrypoint": "index.js",
  "shell": "src/my-app.js",
  "fragments": [
    "src/my-home.js",
    "src/my-awards.js",
    "src/my-talks.js",
    "src/my-stories.js"
  ],
  "sources": [
    "images/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
